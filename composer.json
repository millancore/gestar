{
    "name": "millancore/gestar",
    "description": "Task Manager",
    "type": "project",
    "require": {
        "php": "8.2.*",
        "millancore/xfrmk": "dev-main"
    },
    "require-dev": {
        "pestphp/pest": "^2.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gestar\\": "src/",
            "Tests\\" : "tests/"
        }
    },
    "authors": [
        {
            "name": "Juan Millan",
            "email": "juanes14569@gmail.com"
        }
    ],
    "repositories": [{
        "type": "path",
        "url": "framework"
    }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
